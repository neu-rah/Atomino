{
  "name": "atomino",
  "main": "./lib/atomino",
  "version": "0.4.5",
  "private": false,
  "description": "Atom Editor Arduino-Cosa integration",
  "keywords": [
    "Arduino",
    "AVR",
    "Cosa",
    "Hardware",
    "Firmware",
    "Burn",
    "Upload"
  ],
  "repository": "https://github.com/neu-rah/Atomino",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "packageDependencies": {
    "tool-bar": "0.1.9",
    "console-panel": "0.2.0",
    "terminal-plus": "0.13.1"
  },
  "consumedServices": {
    "console-panel": {
      "activateProviders": true,
      "required": true,
      "versions": {
        "^1.0.0": "consumeConsolePanel"
      }
    },
    "tool-bar": {
      "activateProviders": true,
      "required": true,
      "versions": {
        "^0.1.0": "consumeToolBar"
      }
    }
  }
}
